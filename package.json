{
	"name": "redivis",
	"version": "0.0.2",
	"description": "Redivis client library for javascript",
	"scripts": {
		"test": "node test/index.js",
		"test-browser": "(sleep 1 && open http://127.0.0.1:8000/test/browser) & python3 -m http.server 8000",
		"prepublishOnly": "npm run build && ./node_modules/.bin/bump --tag",
		"build": "./node_modules/.bin/rollup index.js --format cjs --file ./dist/cjs/index.js && ./node_modules/.bin/rollup index.js --format umd --name \"redivis\" --file ./dist/umd/index.js"
	},
	"type": "module",
	"main": "./dist/umd/index.js",
	"module": "./index.js",
	"exports": {
		"import": "./index.js",
		"require": "./dist/cjs/index.js",
		"default": "./index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/redivis/redivis-js.git"
	},
	"keywords": [
		"redivis"
	],
	"author": "Redivis Inc.",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/redivis/redivis-js/issues"
	},
	"homepage": "https://github.com/redivis/redivis-js#readme",
	"dependencies": {
		"keytar": "^7.7.0",
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@skypack/package-check": "0.2.2",
		"prettier": "2.3.2",
		"rollup": "2.54.0",
		"@jsdevtools/version-bump-prompt": "6.1.0"
	},
	"prettier": {
		"printWidth": 120,
		"useTabs": true,
		"tabWidth": 4,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "all",
		"bracketSpacing": true,
		"jsxBracketSameLine": false,
		"arrowParens": "always"
	}
}
